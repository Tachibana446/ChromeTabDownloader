{
    "manifest_version": 2,
    "name": "いろいろダウンローダ",
    "version": "0.1",
    "author": "Tachibana446",
    "description": "開いてる画像タブをまとめて保存とか色々します",
    "browser_action": {
        "default_icon": {
            "38": "icon_38.png"
        },
        "default_title": "ダウンロード！",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "downloads", "http://*/*", "https://*/*"],
    "background": {
        "scripts": ["background.js"],
        "presistent": false
    }
}
